generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  // directUrl = env("DIRECT_URL")
}

model Spin {
  id    String @id @default(uuid())
  name  String
  prize String

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("spins")
}
